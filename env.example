# TNR Business Solutions - Environment Variables
# Copy this file to .env and fill in your actual values

# Wix API Configuration (Required for deployment)
WIX_CLIENT_ID=your_wix_client_id_here
WIX_CLIENT_SECRET=your_wix_client_secret_here
WIX_ACCESS_TOKEN=your_wix_access_token_here

# Email Configuration (for contact forms)
SENDGRID_API_KEY=your_sendgrid_api_key_here
FROM_EMAIL=Info@TNRBusinessSolutions.com
TO_EMAIL=Info@TNRBusinessSolutions.com

# Square Payment Configuration (optional)
SQUARE_ACCESS_TOKEN=your_square_access_token_here
SQUARE_APPLICATION_ID=your_square_app_id_here
SQUARE_ENVIRONMENT=sandbox

# Google Drive Integration (optional)
GOOGLE_DRIVE_CREDENTIALS_FILE=path/to/credentials.json

# OpenAI API (for AI features)
OPENAI_API_KEY=your_openai_api_key_here

# Analytics
GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID

# Security
SECRET_KEY=your_secret_key_for_jwt_tokens
