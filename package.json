{
  "name": "tnr-business-solutions-website",
  "version": "1.0.0",
  "private": true,
  "description": "Professional business website for TNR Business Solutions - Digital Marketing and Insurance Services",
  "scripts": {
    "start": "node serve-optimized.js",
    "serve": "node serve-optimized.js 3000",
    "dev": "node serve-optimized.js 3000",
    "build": "node scripts/simple-build.js",
    "preview": "node serve-optimized.js 3000",
    "deploy:wix": "node scripts/comprehensive-wix-deploy.js",
    "deploy:github": "git add . && git commit -m 'Deploy to Wix' && git push origin main",
    "deploy:full": "npm run build && npm run deploy:wix",
    "wix:auth": "node scripts/wix-deploy.js --auth-only",
    "wix:test": "node scripts/test-wix-connection.js",
    "deploy:auto": "npm run build && node scripts/automated-wix-deploy.js"
  },
  "devDependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.35.0",
    "http-server": "^14.1.1",
    "puppeteer": "^21.3.0"
  },
  "dependencies": {
    "@wix/data": "^1.0.294",
    "@wix/sdk": "^1.17.1",
    "@wix/sites": "^1.0.27",
    "jsdom": "^26.1.0"
  }
}
