# TNR Business Solutions Website - Environment Configuration
# Copy this file to .env and fill in your actual values

# Wix API Configuration
WIX_CLIENT_ID=your_wix_client_id_here
WIX_CLIENT_SECRET=your_wix_client_secret_here
WIX_REFRESH_TOKEN=your_wix_refresh_token_here
WIX_SITE_ID=your_wix_site_id_here

# Wix Data Collections
COLLECTION_SERVICES=Services
COLLECTION_HOMEPAGE=HomepageSections

# Optional: Only push changed content (set to 1 for incremental updates)
ONLY_CHANGED=0

# Backend API Configuration (if using backend services)
BACKEND_URL=http://localhost:8000
API_KEY=your_api_key_here

# Development Settings
NODE_ENV=development
DEBUG=true

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here
